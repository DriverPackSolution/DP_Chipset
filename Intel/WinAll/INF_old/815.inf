[Version]
Signature="$WINDOWS NT$"
Class=System
ClassGuid={4D36E97D-E325-11CE-BFC1-08002BE10318}
Provider=%INTEL%
CatalogFile=815.cat
DriverVer=03/24/2001, 2.30.0017

[ControlFlags]
ExcludeFromSelect=*

[Manufacturer]
%INTEL_MFG%=INTEL_SYS

[INTEL_SYS]
%PCI\VEN_8086&DEV_1130.DeviceDesc%=NO_DRV,PCI\VEN_8086&DEV_1130
%PCI\VEN_8086&DEV_1131.DeviceDesc%=INTEL_AGP440,PCI\VEN_8086&DEV_1131


;**************************************************************************
; AGP Driver Install Section
[INTEL_AGP440]
Include=machine.inf
Needs=AGP440_Install

[INTEL_AGP440.HW]
Include=machine.inf
Needs=AGP440_Install.HW

[INTEL_AGP440.Services]
Include=machine.inf
Needs=AGP440_Install.Services

;**************************************************************************
; Do-nothing section used to avoid "Unknown hardware" installation prompts.
[NO_DRV]
Include=machine.inf
Needs=NO_DRV

[NO_DRV.Services]
Include=machine.inf
Needs=NO_DRV.Services

[Strings]
INTEL         ="Intel"
INTEL_MFG="Intel"
PCI\VEN_8086&DEV_1130.DeviceDesc="Intel(R) 82815 Processor to I/O Controller - 1130"
PCI\VEN_8086&DEV_1131.DeviceDesc="Intel(R) 82815 Processor to AGP Controller - 1131"

